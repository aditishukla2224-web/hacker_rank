#include <stdio.h>
#include <string.h>

#define MAX 1000

char G[MAX][MAX]; // Grid
char P[MAX][MAX]; // Pattern

// Function to check if pattern exists in the grid
char* gridSearch(int R, int C, int r, int c) {
    for (int i = 0; i <= R - r; i++) { // possible starting row
        for (int j = 0; j <= C - c; j++) { // possible starting column
            int found = 1;
            for (int k = 0; k < r; k++) { // check each pattern row
                // Compare substring of G[i+k] starting at j with P[k]
                if (strncmp(G[i + k] + j, P[k], c) != 0) {
                    found = 0;
                    break;
                }
            }
            if (found) {
                return "YES";
            }
        }
    }
    return "NO";
}

int main() {
    int t;
    scanf("%d", &t); // number of test cases

    while (t--) {
        int R, C, r, c;
        scanf("%d %d", &R, &C);
        for (int i = 0; i < R; i++) {
            scanf("%s", G[i]);
        }

        scanf("%d %d", &r, &c);
        for (int i = 0; i < r; i++) {
            scanf("%s", P[i]);
        }

        printf("%s\n", gridSearch(R, C, r, c));
    }

    return 0;
}
