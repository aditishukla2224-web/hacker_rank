#include <stdio.h>
#include <stdlib.h>

int cmpfunc(const void *a, const void *b) {
    int x = *(int *)a;
    int y = *(int *)b;
    return (x > y) - (x < y);
}

int main() {
    int n;
    scanf("%d", &n);
    int *arr = malloc(n * sizeof(int));
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    qsort(arr, n, sizeof(int), cmpfunc);

    int min_diff = abs(arr[1] - arr[0]);
    for (int i = 1; i < n - 1; i++) {
        int diff = abs(arr[i+1] - arr[i]);
        if (diff < min_diff) {
            min_diff = diff;
        }
    }

    for (int i = 0; i < n - 1; i++) {
        if (abs(arr[i+1] - arr[i]) == min_diff) {
            printf("%d %d ", arr[i], arr[i+1]);
        }
    }

    free(arr);
    return 0;
}
