#include <stdio.h>

int gcd(int a, int b) {
    while (b != 0) {
        int temp = b;
        b = a % b;
        a = temp;
    }
    return a;
}

int main() {
    int T;
    scanf("%d", &T);
    while (T--) {
        int a, b, c;
        scanf("%d %d %d", &a, &b, &c);
        if (c <= a || c <= b) {
            int g = gcd(a, b);
            if (c % g == 0)
                printf("YES\n");
            else
                printf("NO\n");
        } else {
            printf("NO\n");
        }
    }
    return 0;
}
